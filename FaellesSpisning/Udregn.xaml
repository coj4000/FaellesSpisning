<Page
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:FaellesSpisning"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ViewModel="using:FaellesSpisning.ViewModel"
    x:Class="FaellesSpisning.Udregn"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <Grid.DataContext>
            <ViewModel:FSViewModel/>
        </Grid.DataContext>
        <Button x:Name="Back" Content="Back                 " HorizontalAlignment="Left" Margin="85,96,0,0" VerticalAlignment="Top" Click="Button_OnClick"/>
        <ListView x:Name="listView" HorizontalAlignment="Left" Height="361" Margin="330,96,0,0" VerticalAlignment="Top" Width="251" ItemsSource="{Binding Samletpris}"/>
        <Button x:Name="Udregning" Content="Udregn" HorizontalAlignment="Left" Margin="221,170,0,0" VerticalAlignment="Top" Command="{Binding HentPrisCommand}" Click="Udregning_Click"/>

    </Grid>
</Page>
